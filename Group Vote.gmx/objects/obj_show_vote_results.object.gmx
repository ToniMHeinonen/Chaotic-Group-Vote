            <string>global.popup = false;
global.round2_items = global.max_points_round_1;
scr_load_vote_items();
starting_x = 200;
starting_y = room_height/2-200;
space = 50;
topic = 75;
names = 150;

var add_votes = 0;
for (var i = 0; i &lt; ds_list_size(name)-1; i++) {
    for (var k = 1; k &lt;= global.voter_amount; k++) {
        add_votes += global.voted_item[k,i]
    }
    whole[| i] = bonus[| i] + extra[| i] + add_votes;
    add_votes = 0;
}


for (var j = 0; j &lt; ds_list_size(name)-1; j++) {
    for (var i = 0; i &lt; ds_list_size(name)-1; i++) {
        if (scr_compare_reals(whole[| i], whole[| i+1]) == -1)  {
            var store_whole = whole[| i];
            var store_name = name[| i];
            var store_bonus = bonus[| i];
            var store_extra = extra[| i];
            whole[| i] = whole[| i+1];
            whole[| i+1] = store_whole;
            name[| i] = name[| i+1];
            name[| i+1] = store_name;
            bonus[| i] = bonus[| i+1];
            bonus[| i+1] = store_bonus;
            extra[| i] = extra[| i+1];
            extra[| i+1] = store_extra;
            //change voted item list too
            for (var k = 1; k &lt;= global.voter_amount; k++) {
                var store_voted_item = global.voted_item[k,i];
                global.voted_item[k,i] = global.voted_item[k,i+1]
                global.voted_item[k,i+1] = store_voted_item;
            }
        }
    }
}


for(var i = 0; i&lt;global.max_points_round_1; i++) {
    item = instance_create(starting_x,starting_y+space*i,obj_results_item);
    item.name = name[| i];
    item.bonus = bonus[| i];
    item.extra = extra[| i];
    item.whole = whole[| i];
    item.position = i;
    //add items which have the same value as last item
    if (i = global.max_points_round_1-1) {
        var j = i;
        while (whole[| j] == whole[| j+1]) {
            item = instance_create(starting_x,starting_y+space*(j+1),obj_results_item);
            item.name = name[| j+1];
            item.bonus = bonus[| j+1];
            item.extra = extra[| j+1];
            item.whole = whole[| j+1];
            item.position = j+1;
            global.round2_items += 1;
            j += 1;
        }
    }
}

scr_save_vote_items();
</string>
