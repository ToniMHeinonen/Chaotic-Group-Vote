            <string>global.popup = false;
scr_load_vote_items();
starting_x = 150;
starting_y = 150;
space = 50;


for (var j = 0; j &lt; ds_list_size(name)-1; j++) {
    for (var i = 0; i &lt; ds_list_size(name)-1; i++) {
        if (scr_compare_strings(name[| i], name[| i+1]) == 1)  {
            var store_whole = whole[| i];
            var store_name = name[| i];
            var store_bonus = bonus[| i];
            var store_extra = extra[| i];
            whole[| i] = whole[| i+1];
            whole[| i+1] = store_whole;
            name[| i] = name[| i+1];
            name[| i+1] = store_name;
            bonus[| i] = bonus[| i+1];
            bonus[| i+1] = store_bonus;
            extra[| i] = extra[| i+1];
            extra[| i+1] = store_extra;
        }
    }
}

var k = 0;
for(var i = 0; i&lt;ds_list_size(name); i++) {
    if (starting_y+space*k &gt; room_height-40) {
        k = 0;
        starting_x += 275;
    }
    item = scr_create_button(starting_x,starting_y+space*k, VOTE_ITEM, "");
    item.old_name = name[| i];
    item.bonus = bonus[| i];
    item.extra = extra[| i];
    item.whole = whole[| i];
    item.position = i;
    //create or reset clicked status
    global.clicked[i] = false;
    k++;
}

scr_save_vote_items();
</string>
